<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

       <description>Gote Farm</description>
       <display-name>Gote Farm</display-name>

        <security-constraint>
            <web-resource-collection>
                <url-pattern>/*</url-pattern>
            </web-resource-collection>
            <auth-constraint>
                <role-name>*</role-name>
            </auth-constraint>
        </security-constraint>

       <listener>
            <listener-class>
                org.springframework.web.context.ContextLoaderListener
            </listener-class>
       </listener>

       <servlet>
               <servlet-name>Gote Farm</servlet-name>
               <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
               <load-on-startup>1</load-on-startup>
       </servlet>

       <servlet-mapping>
               <servlet-name>Gote Farm</servlet-name>
               <url-pattern>/gotefarm/rpc/*</url-pattern>
       </servlet-mapping>

    <servlet>
            <servlet-name>servlets</servlet-name>
            <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
            <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
            <servlet-name>servlets</servlet-name>
            <url-pattern>/gotefarm/servlet/*</url-pattern>
    </servlet-mapping>



</web-app>

